<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>salaryresult page</title>
    <link rel="stylesheet" href="/salaryresult.css">
  </head>
  <body>
    <div class="navbar">
        <img src="/nitlogofinal.png" alt="no image">
        <h1>National Institute of Technology AP: internship project: 4th semester</h3>
    </div>
    <div class="sheader">
        <h1>salary record for employee named "{{user.0.name}}"</h1>
    </div>
    {{#each user}}
    <div class="container">
        <h2 class="dynamicobj">name: <i>{{name}}</i></h2>
        <h2 class="dynamicobj">salary: <i> {{salary}} &#8377</i></h2>
        <h2 class="dynamicobj">month: <i>{{month}}</i></h2>
    </div>
    {{/each}}
   
  </body>
</html>